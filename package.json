        "quasar": "^1.9.10",
        "register-service-worker": "^1.7.0",
        "shortid": "^2.2.15",
        "vue": "^2.6.11",
        "vue-class-component": "^7.2.3",
        "vue-property-decorator": "^8.4.0",
        "vue-router": "^3.1.6",
        "vuex": "^3.1.3"
    },
    "devDependencies": {
        "@types/jest": "^25.1.4",
